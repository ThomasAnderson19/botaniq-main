{
  "expo": {
    "scheme": "ukendtnavn",
    "name": "Ukendtnavn",
    "slug": "Ukendtnavn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#e9f7ef"
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to identify flowers and trees.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to choose existing plant photos."
      },
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
